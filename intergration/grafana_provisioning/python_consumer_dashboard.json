                            },
                            "query": {
                                "params": [
                                    "A",
                                    "5m",
                                    "now"
                                ]
                            },
                            "reducer": {
                                "type": "avg"
                            },
                            "type": "query"
                        }
                    ],
                    "data": [
                        {
                            "refId": "A",
                            "relativeTimeRange": {
                                "from": 300,
                                "to": 0
                            },
                            "datasourceUid": "PROMETHEUS_DS_UID",
                            "model": {
                                "expr": "rate(messages_failed_total[5m])",
                                "interval": "",
                                "legendFormat": "failures/sec",
                                "refId": "A"
                            }
                        }
                    ],
                    "executionErrorState": "alerting",
                    "for": "5m",
                    "frequency": "1m",
                    "handler": 1,
                    "name": "High Message Failure Rate",
                    "noDataState": "no_data",
