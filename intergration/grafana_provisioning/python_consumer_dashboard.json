                "targets": [
                    {
                        "expr": "python_gc_objects_collected_total{generation=\"0\"}",
                        "legendFormat": "gen 0",
                        "refId": "A"
                    },
                    {
                        "expr": "python_gc_objects_collected_total{generation=\"1\"}",
                        "legendFormat": "gen 1",
                        "refId": "B"
                    },
                    {
                        "expr": "python_gc_objects_collected_total{generation=\"2\"}",
                        "legendFormat": "gen 2",
                        "refId": "C"
                    }
